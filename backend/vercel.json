{
  "version": 2,
  "builds": [
    {
      "src": "server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "server.ts"
    },
    {
      "src": "//health",
      "status": 308,
      "headers": {
        "Location": "/health"
      }
    },
    {
      "src": "/(.*)",
      "dest": "server.ts"
    }
  ]
}
